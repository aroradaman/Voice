import socket
MAX = 5000
s = socket.socket(socket.AF_INET,socket.SOCK_STREAM)
s.setsockopt(socket.SOL_SOCKET,socket.SO_REUSEADDR,1)
s.settimeout(1)
try :
	print 'tryinh'
	s.connect(('172.17.20.122',5001))
	print 'connected'
	s.send("hello")
	print 'waiting'
	try :
		print s.recv(MAX)
	except  socket.error as error :
		pass

except socket.error as error :
	print 'Busy'

